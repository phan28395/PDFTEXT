{
  "name": "pdf-to-text-saas",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "tsc --project tsconfig.staging.json && vite build",
    "build:staging:quick": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:staging": "cypress run --config baseUrl=https://staging.yourapp.com",
    "test:e2e:production": "cypress run --config baseUrl=https://yourapp.com",
    "test:all": "npm run test:run && npm run test:integration && npm run test:e2e",
    "test:load": "artillery run load-test.yml",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npx audit-ci --moderate",
    "lighthouse:staging": "lighthouse --preset=desktop --output-path=./lighthouse-staging-report.html",
    "deploy:staging": "node scripts/deploy-staging.js",
    "deploy:staging:quick": "vercel --config vercel.staging.json --env=staging",
    "deploy:production": "node scripts/deploy-production.js",
    "deploy:prod:quick": "vercel --prod",
    "backup:database": "node scripts/database-backup.js",
    "backup:test": "NODE_ENV=test node scripts/database-backup.js",
    "docker:build": "docker build -t pdf-to-text-saas .",
    "docker:run": "docker run -p 3000:3000 pdf-to-text-saas",
    "docker:dev": "docker-compose -f docker-compose.yml --profile development up",
    "health-check": "node health-check.js"
  },
  "dependencies": {
    "@google-cloud/documentai": "^9.3.0",
    "@hookform/resolvers": "^3.3.2",
    "@stripe/stripe-js": "^2.1.11",
    "@supabase/supabase-js": "^2.38.4",
    "@types/formidable": "^3.4.5",
    "@types/multer": "^2.0.0",
    "@types/pdfjs-dist": "^2.10.377",
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "docx": "^9.5.1",
    "formidable": "^3.5.4",
    "json2csv": "^6.0.0-alpha.2",
    "lucide-react": "^0.294.0",
    "multer": "^2.0.2",
    "pdfjs-dist": "^4.8.69",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "stripe": "^14.9.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "artillery": "^2.0.0",
    "audit-ci": "^6.6.1",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "msw": "^2.0.8",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.6",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}
