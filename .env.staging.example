# Staging Environment Variables Template
# Copy this file to .env.staging and fill in your staging values

# Supabase Configuration (Staging)
VITE_SUPABASE_URL=https://your-staging-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your_staging_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_staging_service_role_key_here

# Google Document AI Configuration (Staging)
GOOGLE_APPLICATION_CREDENTIALS=path_to_staging_service_account.json
GOOGLE_CLOUD_PROJECT_ID=your_staging_project_id
GOOGLE_DOCUMENT_AI_LOCATION=us
GOOGLE_DOCUMENT_AI_PROCESSOR_ID=your_staging_processor_id

# Stripe Configuration (Staging - Test Mode)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_staging_webhook_secret_here
STRIPE_PRO_PRICE_ID=price_your_test_pro_price_id

# Security Keys (Generate secure keys for staging)
JWT_SECRET=your_secure_jwt_secret_key_for_staging_minimum_32_chars
ENCRYPTION_KEY=your_secure_encryption_key_for_staging_minimum_32_chars

# VirusTotal API (Staging)
VIRUSTOTAL_API_KEY=your_staging_virustotal_api_key_here

# Staging Configuration
NODE_ENV=staging
VITE_API_URL=https://your-staging-domain.vercel.app

# Staging Rate Limiting (Less restrictive for testing)
RATE_LIMIT_UPLOADS_PER_MINUTE=10
RATE_LIMIT_AUTH_ATTEMPTS=5
RATE_LIMIT_API_REQUESTS_PER_MINUTE=200

# Staging File Processing Configuration
MAX_FILE_SIZE_MB=10
PROCESSING_TIMEOUT_SECONDS=60
TEMP_FILE_CLEANUP_HOURS=2

# Staging Monitoring
LOG_LEVEL=debug
ENABLE_ERROR_TRACKING=true
ENABLE_DEBUG_LOGS=true

# Staging Database Configuration
DATABASE_CONNECTION_POOL_MIN=1
DATABASE_CONNECTION_POOL_MAX=5
DATABASE_STATEMENT_TIMEOUT=30000
DATABASE_IDLE_TIMEOUT=180000

# Staging Security Configuration
CORS_ORIGIN=https://your-staging-domain.vercel.app
SECURE_COOKIE=true
HTTPS_ONLY=true
TRUST_PROXY=true

# Staging Backup Configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE="0 6 * * *"
BACKUP_RETENTION_DAYS=7

# Beta Testing Configuration
BETA_TESTING_ENABLED=true
BETA_FEATURES_ENABLED=true
BETA_USER_FEEDBACK_COLLECTION=true
BETA_ANALYTICS_ENHANCED=true

# Staging-specific Features
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_USER_BEHAVIOR_TRACKING=true
ENABLE_A_B_TESTING=false